{
  "name": "ts-fn",
  "version": "1.0.0",
  "description": "Option and Result types",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "rimraf dist && microbundle",
    "dev": "microbundle watch",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "ts",
    "monad",
    "fp",
    "option",
    "result"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-pattern": "^4.3.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.5.0"
}
